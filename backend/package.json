{
  "name": "backend",
  "version": "1.0.0",
  "description": "Simple Inventory Backend",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development ts-node-dev --respawn src/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:coverage:report": "vitest --coverage --reporter=html",
    "test:coverage:report:open": "vitest --coverage --reporter=html && open coverage/index.html"
  },
  "keywords": [
    "inventory",
    "backend",
    "typescript",
    "express"
  ],
  "author": "Highspot",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.3",
    "supertest": "^7.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "vitest": "^3.1.3"
  }
}
